  transition: opacity 0.3s ease;
}
.fade-enter-from.data-v-57280228,
.fade-leave-to.data-v-57280228 {
  opacity: 0;
}
.slide-up-enter-active.data-v-57280228,
.slide-up-leave-active.data-v-57280228 {
  transition: all 0.3s ease;
}
.slide-up-enter-from.data-v-57280228 {
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  opacity: 0;
}
.slide-up-leave-to.data-v-57280228 {
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  opacity: 0;
}
