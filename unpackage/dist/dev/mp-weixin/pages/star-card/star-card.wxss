}
/* 动画类 */
.fade-enter-active.data-v-70e8f3de,
.fade-leave-active.data-v-70e8f3de {
  transition: opacity 0.3s ease;
}
.fade-enter-from.data-v-70e8f3de,
.fade-leave-to.data-v-70e8f3de {
  opacity: 0;
}
.slide-up-enter-active.data-v-70e8f3de,
.slide-up-leave-active.data-v-70e8f3de {
  transition: all 0.3s ease;
}
.slide-up-enter-from.data-v-70e8f3de {
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  opacity: 0;
}
.slide-up-leave-to.data-v-70e8f3de {
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  opacity: 0;
}
